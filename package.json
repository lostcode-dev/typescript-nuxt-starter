{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "npx nuxi typecheck",
    "lint": "eslint .",
    "lint:eslint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "check-branch-name": "bash ./.husky/validate_current_branch.sh"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx,vue}": [
      "eslint . --fix",
      "eslint .",
      "stylelint ."
    ]
  },
  "dependencies": {
    "@vueuse/core": "^12.2.0",
    "@vueuse/nuxt": "^12.2.0",
    "nuxt": "^3.14.1592",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@nuxt/eslint": "^0.7.4",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "eslint": "^9.17.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "stylelint": "^16.12.0",
    "stylelint-config-clean-order": "^6.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "^5.7.2",
    "vite-plugin-eslint2": "^5.0.3",
    "vue-tsc": "^2.1.10"
  }
}
